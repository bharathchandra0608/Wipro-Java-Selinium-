package JDBC;
 
import java.sql.*;
 
public class DBConnect
{
public static void main(String[] args)
	{
 
	try {
	
		Class.forName("com.mysql.cj.jdbc.Driver");
		Connection con = DriverManager.getConnection("jdbc:mysql://localhost:3306/testdb", "root", "Patel@0608");
		
		System.out.println("Connected Successsfully");
		con.close();
	 }
	catch (Exception e)
	{
	e.printStackTrace();
			
	}
	}
}
 
 